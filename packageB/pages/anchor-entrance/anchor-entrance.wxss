page {
  background: #fff;
}

.slicer {
  width: 100%;
  height: 30rpx;
  background: #f5f5f5;
}

.part-item .title {
  padding: 35rpx 0 50rpx 50rpx;
  font-size: 40rpx;
  align-items: center;
}

.part-item .content-wrap {
  width: 100%;
  min-height: 180rpx;
  padding-bottom: 60rpx;
  background: #fff;
}

.part-item .content-wrap {
  flex-direction: row;
}

.part-item .content-wrap .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  font-size: 26rpx;
}

.part-item .content-wrap .item.row {
  flex-direction: row;
  padding-right: 24rpx;
}

.part-item .content-wrap .item:nth-child(1) {
  padding-left: 96rpx;
}

.cu-color {
  color: #ff6400;
}

.slicer2 {
  height: 15rpx;
}

.ops-wrap {
  padding-left: 96rpx;
}

.ops-wrap .ops-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.ops-wrap navigator:nth-child(2) {
  margin-left: 120rpx;
}

.ops-wrap .ops-item > image {
  width: 112rpx;
  height: 112rpx;
  margin-bottom: 12rpx;
}

.ops-wrap .ops-item  .txt {
  font-size: 28rpx;
  text-align: center;
}

.bundle {
  width: 6rpx;
  height: 32rpx;
  background: rgba(251, 67, 91, 1);
  margin-right: 10rpx;
  border-radius: 3rpx;
}
